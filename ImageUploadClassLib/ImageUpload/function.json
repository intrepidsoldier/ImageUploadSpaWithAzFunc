{
  "scriptFile": "..\\bin\\ImageUploadClassLib.dll",
  "entryPoint": "ImageUploadClassLib.ImageUpload.Run",
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "inputContainer",
      "path": "%INPUT_CONTAINER%",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ],
  "disabled": false
}